{
  "name": "midifile",
  "version": "1.0.4",
  "description": "Read/write standard MIDI files.",
  "main": "src/MIDIFile",
  "author": "Nicolas Froidure",
  "contributors": [
    "M.K. (https://github.com/mk-pmb)"
  ],
  "metapak": {
    "data": {
      "files": "src/*.js tests/*.mocha.js",
      "testsFiles": "tests/*.mocha.js"
    },
    "configs": [
      "readme",
      "mocha",
      "eslint",
      "codeclimate",
      "travis"
    ]
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/nfroidure/MIDIFile.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.9.5"
  },
  "dependencies": {
    "midievents": "^2.0.0",
    "utf-8": "^1.0.0"
  },
  "devDependencies": {
    "browserify": "^12.0.2",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "^1.3.8",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.19.0",
    "eslint-plugin-prettier": "^2.6.0",
    "istanbul": "^0.4.5",
    "metapak": "1.0.3",
    "metapak-nfroidure": "6.0.0",
    "mocha": "^5.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "prettier": "^1.11.1"
  },
  "scripts": {
    "build": "browserify -s MIDIFile src/MIDIFile.js > dist/MIDIFile.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "cli": "env NODE_ENV=${NODE_ENV:-cli}",
    "cover": "istanbul cover _mocha --report html -- tests/*.mocha.js -R spec -t 5000",
    "coveralls": "istanbul cover _mocha --report lcovonly -- tests/*.mocha.js -R spec -t 5000 && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "cz": "env NODE_ENV=${NODE_ENV:-cli} git cz",
    "lint": "eslint src/*.js tests/*.mocha.js",
    "metapak": "metapak",
    "mocha": "mocha tests/*.mocha.js",
    "prettier": "prettier --write src/*.js tests/*.mocha.js",
    "preversion": "npm t && npm run lint && npm run metapak -s",
    "test": "mocha tests/*.mocha.js && npm run mocha",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "keywords": [
    "sound",
    "music",
    "midi",
    "file",
    "format",
    "read",
    "write"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "greenkeeper": {
    "ignore": [
      "commitizen",
      "cz-conventional-changelog",
      "conventional-changelog-cli",
      "mocha",
      "mocha-lcov-reporter",
      "coveralls",
      "istanbul",
      "eslint",
      "eslint-config-prettier",
      "prettier"
    ]
  }
}
